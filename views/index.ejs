    <h1 class="h3 mb-3 fw-normal">Welcome to <%= title %></h1>

    <!-- email -->
    <div class="form-floating">
      <input name="email" type="email" class="form-control" id="floatingInput" placeholder="name@example.com" required>
      <label for="floatingInput">Email address</label>
    </div>
    <!-- first name -->
    <div class="form-floating">
      <input name="firstName" type="firstName" class="form-control" placeholder="First Name" required>
      <label for="floatingfirstName">First Name </label>
    </div>

    <!-- last name -->
    <div class="form-floating">
      <input name="lastName" type="text" class="form-control" placeholder="lastName" required>
      <label for="floatinglastName">Last Name </label>
    </div>

      <!-- Date of Birth -->
      <div class="form-floating"></div>
    <label class="Label" for="dob">Date of Birth</label>
    <input class="noControl" type="date" id="dob" name="dateOfBirth" type="dateOfBirth" class="form-control" placeholder="dateOfBirth" required>
    </div>

    <!-- telephone number  -->
    <div class="form-floating"></div>
      <label for="telephonNumber" class="Label">Enter your phone number:</label>
      <input class="noControl" class="form-control" id="phone" type="number" name="telephonNumber" />
    </div>

    <button id="generator" type="submit" class="btn btn-primary" value="Get QR">Generate</button>
    <p class="mt-5 mb-3 text-muted">&copy; 2017â€“2021</p>
  </form>
</main>

<script>
  const phoneInputField = document.querySelector("#phone");
  const phoneInput = window.intlTelInput(phoneInputField, {
    preferredCountries: ["TR", "SY" , "EG"],
    utilsScript:
      "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",
